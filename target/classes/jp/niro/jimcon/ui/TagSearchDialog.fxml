        <TableView fx:id="tagTable" minHeight="-Infinity" minWidth="-Infinity" scaleShape="false" style="-fx-background-color: null;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
