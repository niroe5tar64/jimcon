                <TableColumn fx:id="tagNameColumn" prefWidth="83.0" text="%tag.tagName" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>
