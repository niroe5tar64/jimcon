<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="jp.niro.jimcon.ui.SupplierSearchDialogController">
    <top>
        <HBox alignment="TOP_CENTER" layoutX="36.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              spacing="5.0" AnchorPane.leftAnchor="35.0">
            <children>
                <TextField fx:id="supplierSearchField">
                    <HBox.margin>
                        <Insets bottom="3.0" top="3.0"/>
                    </HBox.margin>
                </TextField>
                <Button fx:id="supplierSearchButton" mnemonicParsing="false" text="%searchSupplier">
                    <HBox.margin>
                        <Insets bottom="3.0" top="3.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </top>
    <center>
        <TableView fx:id="supplierTable" minHeight="-Infinity" minWidth="-Infinity" scaleShape="false"
                   style="-fx-background-color: null;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="supplierCodeColumn" prefWidth="75.0" text="%supplier.supplierCode"/>
                <TableColumn fx:id="supplierNameColumn" prefWidth="83.0" text="%supplier.supplierName"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>